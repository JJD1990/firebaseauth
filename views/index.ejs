                        aria-current="true" aria-label="Slide 1"></button>
                    <button type="button" data-bs-target="#myCarousel" data-bs-slide-to="1"
                        aria-label="Slide 2"></button>
                    <button type="button" data-bs-target="#myCarousel" data-bs-slide-to="2"
                        aria-label="Slide 3"></button>
                </div>
                <div class="carousel-inner">
                    <div class="carousel-item active img-fluid">
                        <img src="./assets/candle.jpg" alt="" style="object-fit: cover;">

                        <div class="container">
                            <div class="carousel-caption text-start">
                                <h1>Welcome to Eloquin</h1>
                                <p>Holistic Health and Beauty in the heart of Basingstoke</p>
                                <p><a class="btn" id="button" href="about.html">About Us</a></p>
                            </div>
                        </div>
                    </div>
                    <div class="carousel-item img-fluid">
                        <img src="./assets/towels.jpg" alt="" style="object-fit: cover;">


                        <div class="container">
                            <div class="carousel-caption">
                                <h1 id="navy-text">Treatments</h1>
                                <p id="navy-text">Everything you deserve, and more. </p>
                                <p><a class="btn" id="button" href="treatments.html">View our Treatments</a></p>
                            </div>
                        </div>
                    </div>
                    <div class="carousel-item img-fluid">
                        <img src="./assets/rocks.jpg" alt="" style="object-fit: cover;">


                        <div class="container">
                            <div class="carousel-caption text-end">
                                <h1>Reviews</h1>
                                <p>Dont just take our word for it!</p>
                                <p><a class="btn" id="button" href="reviews.html">See Reviews</a></p>
                            </div>
                        </div>
                    </div>
                </div>
                <button class="carousel-control-prev" type="button" data-bs-target="#myCarousel" data-bs-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#myCarousel" data-bs-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Next</span>
                </button>
            </div>


